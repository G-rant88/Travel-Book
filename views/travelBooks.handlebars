<div class="container">
  {{!-- page title --}}
  <div class="col s12 center-align">
    <h3 class="names"> Posts!</h3>
  </div>
  {{!-- results data --}}
  <div class="row">

    {{#each userPost.data}}
    <div class="col s12 m4 center-align">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title">Country: {{dataValues.country}}</span>
          <span class="card-title">City: {{dataValues.city}}</span>
          <span class="card-title">Place: {{dataValues.name}}</span>
          <img src=https://s3-us-west-2.amazonaws.com/travelbookpictures/{{dataValues.image}} width="100%">
          <p>Review: {{dataValues.review}}</p>
          <p><span class="rating">Rating: {{dataValues.rating}}</span></p>
          <p>Price: {{dataValues.price}}</p>
          <p>Category: {{dataValues.categories}}</p>
          <!-- <p>Written By: {{user.username}}</p> -->
        </div>
           <div class="card-action">
              {{!-- change icon based on category; icons: local_dining, local_taxi, local_hotel--}}
              <a href="#" class="like-btn" data-status="unliked" data-post-id="{{id}}" data-name="{{name}}" data-price="{{price}}" data-rating="{{rating}}" data-categories="{{categories}}" data-review={{review}} data-image="{{image}}"><i class="material-icons heart">favorite_border</i></a>

              {{!-- change icon based on category; icons: local_dining, local_taxi, local_hotel--}}
              <a href="#" class="delete-btn" data-post-id="{{dataValues.id}}" data-name="{{name}}" data-price="{{price}}" data-rating="{{rating}}" data-categories="{{categories}}" data-review={{review}} data-image="{{image}}"><i class="material-icons">clear</i></a>
      
              {{!-- change icon based on category; icons: local_dining, local_taxi, local_hotel--}}
              <a href="#" class="edit-btn" data-post-id="{{dataValues.id}}" data-name="{{name}}" data-price="{{price}}" data-rating="{{rating}}" data-categories="{{categories}}" data-review={{review}} data-image="{{image}}"><i class="material-icons">create</i></a>
            </div>
      </div>
    </div>
    {{/each}}
  </div>

   {{!-- liked list as sidenav --}}
    <ul id="slide-out" class="side-nav">
        <li>
          <h6>For My Trip</h6>
        </li>
        {{!-- selected items go here as li tags --}}
    </ul>
    {{!-- added list as sidenav --}}    
    {{!-- contains buttons for viewing lists and submitting lists --}}
    <div class="fixed-action-btn vertical">
    <a class="btn-floating btn-large">
      <i class="large material-icons">mode_edit</i>
    </a>
      <ul>
        {{!-- shows liked list --}}
        <li><a class="btn-floating red button-collapse" id="liked-list-btn" data-activates="slide-out"><i class="material-icons">loyalty</i></a></li>
        {{!-- shows added list --}}
       
        {{!-- triggers naming modal --}}
        <li><a class="btn-floating blue modal-trigger" href="#naming-trip" id="modal-btn"><i class="material-icons">send</i></a></li>
      </ul>
    </div>

    {{!-- modal for naming a previous trip and/or new trip --}}
    <!-- Modal Structure -->
    <div id="naming-trip" class="modal">
      <div class="modal-content">
        <h4>Great! Now, what is the name of your trip?</h4>
        {{!-- name new trip --}}
        <div class="input-field col s6">
          <i class="material-icons prefix">mode_edit</i>
          <input class="previous-trip-name" id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Trip Name</label>
          <a class="waves-effect waves-light btn submit-list-btn">Submit</a>
        </div>
        {{!-- name previous trip --}}
  <!--       <div class="input-field col s6">
          <i class="material-icons prefix">mode_edit</i>
          <input class="future-trip-name" id="icon_prefix" type="text" class="validate">
          <label for="icon_prefix">Future Trip Name</label>
        </div>
        
      </div>
    </div> -->
          

</div>
</div>
{{>login}}