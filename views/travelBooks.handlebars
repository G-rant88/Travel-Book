<div class="container">
    {{!-- page title --}}
    <div class="col s12 center-align">
        <h3 class="names"> Travel Book!</h3>
    </div>
    {{!-- results data --}}
    <div class="row">
      {{#each data.daty}}
       <div class="col s12 m4 center-align">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">Place: {{name}}</span>
              <img src=https://s3-us-west-2.amazonaws.com/travelbookpictures/{{image}} width="100%">
              <p>Review: {{review}}</p>
              <p><span class="rating">Rating: {{rating}}</span></p>
              <p>Price: {{price}}</p>
              <p>Written By: {{user.username}}</p>
            </div>
            <div class="card-action">
              {{!-- change icon based on category; icons: local_dining, local_taxi, local_hotel--}}
              <a href="#" class="add-btn" data-status="unadded" data-post-id="{{id}}" data-name="{{name}}" data-price="{{price}}" data-rating="{{rating}}" data-categories="{{categories}}"><i class="material-icons add">add_circle_outline</i></a>
              <a href="#" class="like-btn" data-status="unliked" data-post-id="{{id}}" data-name="{{name}}" data-price="{{price}}" data-rating="{{rating}}" data-categories="{{categories}}"><i class="material-icons heart">favorite_border</i></a>
            </div>
          </div>
      </div>
      {{/each}}
    </div>
  
          

</div>

{{>login}}