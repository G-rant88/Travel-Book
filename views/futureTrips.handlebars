<div class="container">
    {{!-- page title --}}

    <div class="col s12 center-align">
        <h3 class="names"> Travel Book!</h3>
    </div>
    {{#each data.daty}}
    <div class="row">
    <div class="col s12 m4 left-align left">

    <h4>Trip: {{tripName}}</h4>
    </div>
    </div>

    <div class="row">
         <div class="col s4 m4 center-align">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">Country: {{post.country}}</span>
              <span class="card-title">City: {{post.city}}</span>
              <span class="card-title">Place: {{post.name}}</span>
              <img src=https://s3-us-west-2.amazonaws.com/travelbookpictures/{{post.image}} width="100%">
              <p>Review: {{post.review}}</p>
              <p><span class="rating">Rating: {{post.rating}}</span></p>
              <p>Price: {{post.price}}</p>
              <p>Category: {{post.categories}}</p>
             <!--  <p>Written By: {{username}}</p> -->
            </div>
            <div class="card-action">
              {{!-- change icon based on category; icons: local_dining, local_taxi, local_hotel--}}
              <a href="#" class="clear-btn" data-post-id="{{post.id}}" data-name="{{name}}" data-price="{{price}}" data-rating="{{rating}}" data-categories="{{categories}}" data-review={{review}} data-image="{{image}}"><i class="material-icons heart">clear</i></a>
            </div>

          </div>
          
      </div>
{{/each}}
      
</div>

{{>login}}