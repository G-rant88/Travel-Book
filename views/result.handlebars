<nav>
    <div class="nav-wrapper">
        <a href="#" class="brand-logo">Travel Tips</a>
        <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li><a href="#">Trips You've Taken</a></li>
        <li><a href="#">Your Saved Trips</a></li>
        <li><a href="#">Plan New Trip</a></li>
        <li><a id="login">Login</a></li>
        </ul>
    </div>
</nav>

<div class="container">
    {{!-- page title --}}
    <div class="col s12 center-align">
        <h3>Search Results!</h3>
    </div>
    <div class="row">
    	{{#each results}}
    	 <div class="col s12 m6">
          <div class="card blue-grey darken-1">
            <div class="card-content white-text">
              <span class="card-title">{{thing}}</span>
              <img src={{image}} height="350" width="500">
              <p>{{review}}</p>
              <p>{{rating}}</p>
              <p>{{price}}</p>
            </div>
            <div class="card-action">
              <a href="#">Add to Trip</a>
              <a href="#">Like</a>
            </div>
          </div>
    </div>
		{{/each}}
    </div>